  "summary": "An API for displaying iOS action sheets and share sheets.",
  "homepage": "http://facebook.github.io/react-native/",
  "license": "MIT",
  "authors": "Facebook, Inc. and its affiliates",
  "platforms": {
    "ios": "9.0",
    "tvos": "9.2"
  },
  "source": {
